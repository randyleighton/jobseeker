<h3><%= @contact.first_name + " " + @contact.last_name %></h3>

<p>
  <b>Email:</b>
  <%=h @contact.email %>
</p>

<p>
  <b>Phone:</b>
  <%=h @contact.work_phone %>
  <%=h @contact.cell_phone %>
</p>

<p>
  <b>Company:</b>
  <%= link_to @contact.company.name, company_path(@contact.company) %>
</p>

<div class="margin-top">
  <div class="show-edit-del">
    <%= link_to edit_company_contact_path(@contact.company, @contact) do %>
      <%= image_tag("edit.png") %>
      <p>Edit</p>
    <% end %>
  </div>
  <div class="show-edit-del">
    <%= link_to [@contact.company, @contact], data: {method: :delete, confirm: 'Are you sure you want to delete this contact?' } do %>
      <%= image_tag("trash-holiday.png") %>
      <p>Delete</p>
    <% end %>
  </div>
  <div class="show-edit-del">
  <%= link_to company_contacts_path do %>
    <%= image_tag("navigation.png") %>
     <p>Back</p>
  <% end %>
  </div>
</div>